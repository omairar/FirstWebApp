
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Request Object</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/Site.css" rel="stylesheet">

    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,700;0,800;0,900;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
        <div class="container-fluid">

            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarColor02">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link " aria-current="page" href="/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link " href="/Actionss/Index">Action</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link " href="/RazorExpressions/Index">Razor Expressions</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="/HttpObjects/Index">HTTP Objects</a>
                    </li>

                </ul>

            </div>
        </div>
    </nav>
    <div class="container pt-5 d-flex">
        <table class="table table-success table-striped">
            <thead>
                <tr>
                    <th scope="col">Method</th>
                    <th scope="col">Values</th>
                    
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">Request.Url</th>
                    <td>
                        @ViewBag.reqURL

                    </td>

                </tr>
                <tr>
                    <th scope="row">Request.PhysicalApplicationPath</th>
                    <td>
                        @ViewBag.reqPAP

                    </td>

                </tr>
                <tr>
                    <th scope="row">Request.Path</th>
                    <td>
                        @ViewBag.reqP
                    </td>

                </tr>
                <tr>
                    <th scope="row">Request.Browser.Type</th>
                    <td>
                        @ViewBag.reqBT  
                    </td>

                </tr>
                <tr>
                    <th scope="row">Request.QueryString["n"]</th>
                    <td>
                        @ViewBag.reqQS 
                    </td>

                </tr>
                <tr>
                    <th scope="row">Request.Headers["Accept"]</th>
                    <td>
                        @ViewBag.reqH 
                
                    </td>

                </tr>
                <tr>
                    <th scope="row">Request.HttpMethod</th>
                    <td>
                       
                        @ViewBag.reqHTM 
                    </td>

                </tr>
            </tbody>
        </table>


    </div>

    <script src="~/Content/bootstrap.bundle.min.js"></script>
</body>
</html>
